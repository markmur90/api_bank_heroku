              "required": false,
              "schema": {
                "maxLength": 50,
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "totalItems": {
                        "minimum": 0,
                        "type": "integer",
                        "description": "Cumulated number of items across all pages for your request parameters.",
                        "format": "int32"
                      },
                      "limit": {
                        "minimum": 0,
                        "type": "integer",
                        "description": "The number of items of a page.",
                        "format": "int32"
